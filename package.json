  },
  "homepage": "https://github.com/zebing/babel-plugin-vue-css-modules#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/types": "^7.12.12",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-url": "^6.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "rollup": "^2.35.1",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.1.0"
