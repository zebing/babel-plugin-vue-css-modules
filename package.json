{
  "name": "@zebing/babel-plugin-vue-css-modules",
  "version": "1.0.0",
  "description": "vue css modules智能转化插件",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "build": "del dist && cross-env NODE_ENV=production rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,json}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zebing/babel-plugin-vue-css-modules.git"
